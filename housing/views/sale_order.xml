<?xml version="1.0" encoding="UTF-8"?>     
<odoo>
    <data>
        <record id="view_sale_order_venta_actual_tree" model="ir.ui.view">
            <field name="name">inherit.sale.tree</field>
            <field name="model">sale.order</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <tree string="venta_actual" create="false" edit="false">
                    <field name="name"/>
                    <field name="state" invisible="0"/>
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="action_view_sale_order_venta_actual">
            <field name="name">Venta actual</field>
            <field name="res_model">sale.order</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="housing.view_sale_order_venta_actual_tree"/>
            <field name="domain">[('date_order','>=',((context_today())).strftime('%Y-%m-%d 00:00:00')), ('date_order', '&lt;=',((context_today())).strftime('%Y-%m-%d 23:59:59'))]</field>
            <field name="context"></field>
        </record>

        

    </data>
</odoo>